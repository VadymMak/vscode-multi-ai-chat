{
  "name": "vscode-multi-ai-chat",
  "version": "1.0.0",
  "description": "A VS Code extension for multi-AI chat integration.",
  "main": "dist/extension.js",
  "scripts": {
    "compile": "tsc -p ./",
    "build:webview": "cd webview-ui && npm run build",
    "build": "npm run compile && npm run build:webview",
    "package": "vsce package",
    "test": "jest"
  },
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.0",
    "@types/vscode": "^1.74.0",
    "jest": "^29.5.0",
    "typescript": "^5.0.0",
    "vite": "^7.2.7",
    "webpack": "^5.88.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "date-fn": "0.0.2",
    "date-fns": "4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/vscode-multi-ai-chat.git"
  },
  "bugs": {
    "url": "https://github.com/your-repo/vscode-multi-ai-chat/issues"
  },
  "homepage": "https://github.com/your-repo/vscode-multi-ai-chat#readme",
  "categories": [
    "Other"
  ],
  "files": [
    "dist",
    "out",
    "webview-ui/dist",
    "resources"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "multi-ai-chat-sidebar",
          "title": "Multi AI Chat",
          "icon": "$(comment-discussion)"
        }
      ]
    },
    "views": {
      "multi-ai-chat-sidebar": [
        {
          "type": "webview",
          "id": "multi-ai-chat.mainView",
          "name": "Chat",
          "contextualTitle": "Multi AI Chat",
          "icon": "$(comment-discussion)",
          "options": {
            "enableScripts": true,
            "localResourceRoots": [
              "webview-ui/dist"
            ],
            "portMapping": [
              {
                "webviewPort": 5173,
                "extensionHostPort": 5173
              }
            ]
          }
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-multi-ai-chat.start",
        "title": "Multi AI Chat: Start"
      },
      {
        "command": "vscode-multi-ai-chat.indexWorkspace",
        "title": "Multi AI Chat: Index Workspace"
      },
      {
        "command": "multi-ai-chat.findRelatedFiles",
        "title": "Multi AI Chat: Find Related Files"
      },
      {
        "command": "multi-ai-chat.explainFile",
        "title": "Multi AI Chat: Explain File"
      },
      {
        "command": "multi-ai-chat.showDependencies",
        "title": "Multi AI Chat: Show Dependencies"
      },
      {
        "command": "multi-ai-chat.findError",
        "title": "Multi AI Chat: Find Error"
      },
      {
        "command": "multi-ai-chat.editFile",
        "title": "AI: Edit This File",
        "category": "Multi-AI Chat",
        "icon": "$(edit)"
      },
      {
        "command": "multi-ai-chat.focus",
        "title": "Focus on Multi AI Chat",
        "category": "Multi AI Chat"
      },
      {
        "command": "multi-ai-chat.copyContextForAI",
        "title": "Smart Cline: Copy Context for AI Chat"
      },
      {
        "command": "multi-ai-chat.copyCurrentFileForAI",
        "title": "Smart Cline: Copy Current File for AI"
      }
    ],
    "keybindings": [
      {
        "command": "multi-ai-chat.findRelatedFiles",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "editorTextFocus"
      },
      {
        "command": "multi-ai-chat.explainFile",
        "key": "ctrl+shift+e",
        "mac": "cmd+shift+e",
        "when": "editorTextFocus"
      },
      {
        "command": "multi-ai-chat.showDependencies",
        "key": "ctrl+shift+d",
        "mac": "cmd+shift+d",
        "when": "editorTextFocus"
      },
      {
        "command": "multi-ai-chat.findError",
        "key": "ctrl+shift+x",
        "mac": "cmd+shift+x",
        "when": "editorTextFocus"
      },
      {
        "command": "multi-ai-chat.editFile",
        "key": "ctrl+shift+a",
        "mac": "cmd+shift+a",
        "when": "editorTextFocus"
      },
      {
        "command": "multi-ai-chat.copyContextForAI",
        "key": "ctrl+shift+c",
        "mac": "cmd+shift+c"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "multi-ai-chat.editFile",
          "when": "editorTextFocus",
          "group": "multi-ai-chat@1"
        }
      ]
    }
  },
  "activationEvents": [
    "onStartupFinished"
  ]
}
