{
  "name": "vscode-multi-ai-chat",
  "version": "1.0.0",
  "description": "A VS Code extension for multi-AI chat integration.",
  "main": "dist/extension.js",
  "scripts": {
    "compile": "tsc -p ./",
    "build:webview": "vite build",
    "package": "vsce package",
    "test": "jest"
  },
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "18.x",
    "@types/vscode": "^1.74.0",
    "jest": "^29.5.0",
    "typescript": "^5.0.0",
    "vite": "^4.3.9",
    "webpack": "^5.88.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "date-fns": "^4.1.0"
  },
  "vsce": {
    "categories": [
      "Other"
    ],
    "icon": "images/icon.png",
    "repository": {
      "type": "git",
      "url": "https://github.com/your-repo/vscode-multi-ai-chat.git"
    },
    "bugs": {
      "url": "https://github.com/your-repo/vscode-multi-ai-chat/issues"
    },
    "homepage": "https://github.com/your-repo/vscode-multi-ai-chat#readme"
  },
  "files": [
    "dist",
    "out",
    "webview-ui/dist"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-multi-ai-chat.start",
        "title": "Multi AI Chat: Start"
      },
      {
        "command": "vscode-multi-ai-chat.openMainPanel",
        "title": "Multi AI Chat: Open Panel"
      }
    ]
  },
  "activationEvents": [
    "onStartupFinished"
  ]
}
